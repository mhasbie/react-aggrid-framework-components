{
  "name": "react-aggrid-framework-components",
  "version": "0.1.0",
  "description": "Ag-Grid custom built framework components for ag-grid-react. Select, Multi-select filter components for Ag-Grid.",
  "main": "dist/react-aggrid-framework-components.js",
  "scripts": {
    "build": "npm run build:lib && npm run build:dist",
    "build:dist": "webpack src/index.js dist/react-aggrid-framework-components.js & webpack -p src/index.js dist/react-aggrid-framework-components.min.js",
    "build:lib": "npm run compile:lib",
    "clean:lib": "rm -Rf ./lib",
    "compile:lib": "babel src --out-dir lib",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mhasbie/react-aggrid-framework-components.git"
  },
  "keywords": [
    "react",
    "aggrid",
    "ag-grid",
    "react-aggrid",
    "react-aggrid-framework",
    "react-aggrid-framework-components",
    "select filter",
    "set filter",
    "multiselect filter",
    "multi-select filter",
    "ag-grid-react"
  ],
  "author": "M. Hasbie",
  "license": "MIT",
  "peerDependencies": {
    "ag-grid": "^16.0.1",
    "ag-grid-react": "^16.0.0",
    "react": "^15.6.1",
    "react-dom-factories": "^1.0.2",
    "react-bootstrap-typeahead": "^1.4.0",
    "underscore": "^1.8.3",
    "underscore.string": "^3.3.4"
  },
  "devDependencies": {
    "babel-cli": "^6.8.0",
    "babel-core": "^6.8.0",
    "babel-eslint": "^7.0.0",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.0.15",
    "babel-preset-stage-0": "^6.1.18",
    "babel-register": "^6.26.0",
    "eslint": "^3.8.1",
    "eslint-plugin-react": "^6.4.1",
    "lodash.isequal": "^4.4.0",
    "prop-types": "^15.5.10",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "ag-grid": "^16.0.1",
    "ag-grid-react": "^16.0.0",
    "react-dom-factories": "^1.0.2",
    "react-bootstrap-typeahead": "^1.4.0",
    "underscore": "^1.8.3",
    "underscore.string": "^3.3.4",
